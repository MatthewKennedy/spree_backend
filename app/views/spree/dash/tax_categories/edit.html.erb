<% content_for :page_title do %>
  <%= link_to Spree.t(:tax_categories), spree.dash_tax_categories_url %> /
  <%= @tax_category.name %>
<% end %>

<%= render partial: 'spree/dash/shared/error_messages', locals: { target: @tax_category } %>

<%= form_with model: [:dash, @tax_category] do |f| %>
  <fieldset>
    <%= render partial: 'form', locals: { f: f } %>
    <%= render partial: 'spree/dash/shared/edit_resource_links' %>
  </fieldset>
<% end %>
