<% content_for :page_title do %>
  <%= link_to Spree.t(:stores), spree.dash_stores_path(@store) %> /
  <%= Spree.t(:add_store) %>
<% end %>

<%= form_with model: @store, url: spree.dash_stores_path(@store), data: { turbo: false } do |f| %>
  <fieldset>
    <%= render partial: 'form', locals: { f: f } %>
    <%= render partial: 'spree/dash/shared/new_resource_links', locals: { collection_url: spree.dash_stores_path(@store) } %>
  </fieldset>
<% end %>
