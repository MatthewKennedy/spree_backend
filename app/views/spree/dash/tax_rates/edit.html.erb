<% content_for :page_title do %>
  <%= link_to Spree.t(:tax_rates), spree.dash_tax_rates_url %> /
  <%= @tax_rate.name %>
<% end %>

<%= render partial: 'spree/dash/shared/error_messages', locals: { target: @tax_rate } %>

<%= form_with model: [:dash, @tax_rate] do |f| %>
  <%= render partial: 'form', locals: { f: f } %>
  <%= render partial: 'spree/dash/shared/edit_resource_links' %>
<% end %>
