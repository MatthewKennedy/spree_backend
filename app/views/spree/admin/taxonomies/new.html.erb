<% content_for :page_title do %>
  <%= link_to I18n.t('spree.dash.taxonomies.categories'), spree.admin_taxonomies_url %> / <%= I18n.t('spree.dash.new_category') %>
<% end %>
<%= render partial: 'spree/admin/shared/error_messages', locals: { target: @taxonomy } %>

<%= form_with model: [:admin, @taxonomy] do |f| %>
  <fieldset>
    <%= render partial: 'form', locals: { f: f } %>
    <%= render partial: 'spree/admin/shared/new_resource_links' %>
  </fieldset>
<% end %>
