<div class="form-actions" data-hook="buttons">
  <%= button I18n.t('spree.dash.actions.create'), 'plus-lg.svg', 'submit', {class: 'btn-success ', data: { disable_with: "#{ Spree.t(:saving) }..." }} %>
  <span class="or">
    <%= Spree.t(:or) %>
  </span>
  <%= link_to_with_icon(I18n.t('spree.dash.actions.cancel'), collection_url, {class: 'btn btn-light', icon: 'x-lg.svg'}) %>
</div>
